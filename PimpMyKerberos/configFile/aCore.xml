<pimpmykerberos.core.Core>
  <coreFile>./configFile/aCore.xml</coreFile>
  <webServerPort>443</webServerPort>
  <webServerIp>0.0.0.0</webServerIp>
  <webSocketPort>4430</webSocketPort>
  <debug>true</debug>
  <deepLearning>true</deepLearning>
  
  <timeBetweenScan>60000</timeBetweenScan>
  <debugJetty>false</debugJetty>
  <adminPassword>admin</adminPassword>
  <maxLogEntries>1000</maxLogEntries>
  <kerberosioPath>/kerberos</kerberosioPath>
  <maxDisplayLineInGUI>5</maxDisplayLineInGUI>
  <daysBeforePurge>30</daysBeforePurge>
  <daysBeforePurgeKeeped>365</daysBeforePurgeKeeped>
  <maxDisplayColumnInGUI>6</maxDisplayColumnInGUI>
  <nightMode>1</nightMode>
  <muteMode>0</muteMode>
  <http2>true</http2>
  <sessionId>0</sessionId>
  <users>
    <entry>
      <string>admin</string>
      <pimpmykerberos.beans.User>
        <description>Administrator</description>
        <name>admin</name>
        <password>admin</password>
      </pimpmykerberos.beans.User>
    </entry>
  </users>
  <dataPath></dataPath>
  <influxDbUrl></influxDbUrl>
  <influxDbName>pimpMyKerberos</influxDbName>
  <influxDbUser>root</influxDbUser>
  <influxDbPasswd>root</influxDbPasswd>
</pimpmykerberos.core.Core>
